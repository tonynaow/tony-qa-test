  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "wdio": "wdio run ./wdio.conf.ts",
    "format": "prettier --write \"**/*.{ts,js,json,feature}\"",
    "lint": "eslint \"test/**/*.{ts,js}\" wdio.conf.ts --max-warnings=0",
